{
  "name": "sat-digital-backend",
  "version": "1.0.0",
  "description": "SAT-Digital Backend - Sistema de Auditorías Técnicas Digitalizado",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "node src/shared/database/migrate.js",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "node src/shared/database/seeders.js",
    "db:reset": "npm run migrate && npm run seed",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "setup:comunicacion": "node setup-comunicacion.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "health-check": "node src/shared/scripts/healthCheck.js"
  },
  "keywords": [
    "auditorias",
    "tecnicas",
    "infraestructura",
    "datacenter",
    "ia",
    "ollama",
    "mysql",
    "nodejs",
    "websocket",
    "chat"
  ],
  "author": "SAT-Digital Team",
  "license": "MIT",
  "dependencies": {
    "agenda": "^5.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.18",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
