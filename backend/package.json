    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.2",
    "redis": "^5.8.2",
    "sequelize": "^6.32.1",
    "sharp": "^0.32.6",
    "socket.io": "^4.8.1",
    "tedious": "^18.6.1",
    "winston": "^3.10.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "babel-jest": "^29.6.4",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
